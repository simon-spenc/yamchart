{
  "name": "@dashbook/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/exports.js",
  "exports": {
    ".": {
      "types": "./dist/exports.d.ts",
      "import": "./dist/exports.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
