    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@dashbook/config": "workspace:*",
    "@types/node": "^22.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
